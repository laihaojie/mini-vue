{
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "eslint.codeAction.showDocumentation": {
        "enable": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "javascriptreact",
        "html",
        "typescriptreact",
        "vue"
    ],
}